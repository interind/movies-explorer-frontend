{
 "extends": ["eslint:recommended", "plugin:react/recommended", "airbnb-base", "plugin:react-hooks/recommended"],
 "env": {
        "browser": true,
        "node": true,
        "commonjs": true,
        "es6": true
    },
    "plugins": [
    "react"
  ],
 "rules": {
   "no-underscore-dangle":"off",
   "class-methods-use-this": "off",
   "react/jsx-uses-react": "error",
    "react/jsx-uses-vars": "error",
    "react-hooks/rules-of-hooks": "error",
    "react-hooks/exhaustive-deps": "warn",
    "no-console": "warn",
    "linebreak-style": "off"
 },
 "parser": "babel-eslint",
 "parserOptions": {
    "ecmaFeatures": {
      "jsx": true
    },
    "ecmaVersion": 2021,
    "sourceType": "module"
  }
}